<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.green.spring.dao.MemberDAO">
  <select id="selectEmail" resultType="String">
  	select me_email from member where me_id = #{me_id}
  </select>
  <select id="selectMember" resultType="kr.green.spring.vo.MemberVO">
  	select * from member where me_id = #{me_id}
  </select>
  <select id="selectMember2" resultType="kr.green.spring.vo.MemberVO">
  	select * from member where me_id = #{member.me_id} and me_pw = #{member.me_pw}
  </select>
</mapper>
